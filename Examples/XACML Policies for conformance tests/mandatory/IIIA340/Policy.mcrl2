
	sort SAtt = struct attribute(name:SAttName, value:SAttValue);
	sort SAttName = struct
		subjectid;
	sort SAttValue = struct 	
		JuliusHibbert;
	
	sort EAtt = struct attribute(name:EAttName, value:EAttValue);
	
	sort EAttName = struct 
		NaN|
				INF|
				NegativeINF;
	
	sort EAttValue = struct
		;
	

	
	
	




							
	sort Decision = struct Permit | Deny;
	


							
	
		sort ObgID = struct 
		obligation1;
				
	


		
							
	act
		Request:
			FSet(SAtt)#FSet(EAtt);
		
		Obligation:
			FSet(SAtt)#FSet(EAtt)#ObgID;
				
		Response:
			FSet(SAtt)#FSet(EAtt)#Decision;
	


	



		
	
	proc	
		
	
	Policy_policy(
			RS:FSet(SAtt), RE:FSet(EAtt)) = 
	
			
		
			Rule_rule(
			RS, RE);
			
	
		Rule_rule(
			RS:FSet(SAtt), RE:FSet(EAtt))=
		
		
		
					(attribute(subjectid, JuliusHibbert) in 
					
				RS) 
			 ->  
						
				Obligation(
			RS, RE, obligation1).
			
		

		Response(
			RS, RE,Permit);
	
	


		



		init 
			sum RS:FSet(SAtt).sum RE:FSet(EAtt).(
			RS !={}&& RE !={})-> Request(
			RS, RE).Policy_policy(
			RS, RE);	
	