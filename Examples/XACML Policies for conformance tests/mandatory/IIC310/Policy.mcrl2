
	sort SAtt = struct attribute(name:SAttName, value:SAttValue);
	sort SAttName = struct
		subjectid;
	sort SAttValue = struct 	
		bert;
	

	
	
	




							
	sort Decision = struct Permit | Deny;
	


							
			
	


		
							
	act
		Request:
			FSet(SAtt);
				
		Response:
			FSet(SAtt)#Decision;
	


	



		
	
	proc	
		
	
	Policy_policy(
			RS:FSet(SAtt)) = 
	
			
		
			Rule_rule(
			RS);
			
	
		Rule_rule(
			RS:FSet(SAtt))=
		
		
		
					(attribute(subjectid, bert) in 
					
				RS) 
			 ->  
					
		

		Response(
			RS,Permit);
	
	


		



		init 
			sum RS:FSet(SAtt).(
			RS !={})-> Request(
			RS).Policy_policy(
			RS);	
	