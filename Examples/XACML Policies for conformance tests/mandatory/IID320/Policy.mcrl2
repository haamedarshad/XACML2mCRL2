
	sort SAtt = struct attribute(name:SAttName, value:SAttValue);
	sort SAttName = struct
		subjectid|
				age|
				test;
	sort SAttValue = struct 	
		JHibbert|
			|
			ZaphodBeedlebrox;
	
	sort EAtt = struct attribute(name:EAttName, value:EAttValue);
	
	sort EAttName = struct 
		bartsimpsonage;
	
	sort EAttValue = struct
		;
	

	
	
	




							
	sort Decision = struct Permit | Deny;
	


							
			
	


		
							
	act
		Request:
			FSet(SAtt)#FSet(EAtt);
				
		Response:
			FSet(SAtt)#FSet(EAtt)#Decision;
	


	



		
	
	proc	
		
	
			PolicySet_policyset(
			RS:FSet(SAtt), RE:FSet(EAtt)) = 


					
						
					Policy_policy1(
			RS, RE)+	
							
					Policy_policy2(
			RS, RE)+	
					
					Policy_policy3(
			RS, RE);	
				
		
			Policy_policy1(
			RS:FSet(SAtt), RE:FSet(EAtt))=	
			
					
							
		(	
		
				(
						
					(attribute(subjectid,JHibbert) in 
					
				RS) 
			
				)		
				
		)->				
		
					Rule_rule1(
			RS, RE);
				
		
			Policy_policy2(
			RS:FSet(SAtt), RE:FSet(EAtt))=	
			
					
				
					Rule_rule2(
			RS, RE);
				
		
			Policy_policy3(
			RS:FSet(SAtt), RE:FSet(EAtt))=	
			
					
				
					Rule_rule3(
			RS, RE);
					
		
			Rule_rule1(
			RS:FSet(SAtt), RE:FSet(EAtt))=
			
			
			
			
			
			Response(
			RS, RE, Deny);
			
		
			Rule_rule2(
			RS:FSet(SAtt), RE:FSet(EAtt))=
			
			
			
			
			
			Response(
			RS, RE, Permit);
			
		
			Rule_rule3(
			RS:FSet(SAtt), RE:FSet(EAtt))=
			
			
			
					(attribute(test, ZaphodBeedlebrox) in 
					
				RS) 
			 ->  
					
			
			
			Response(
			RS, RE, Permit);
		
		





		



	init 
			sum RS:FSet(SAtt).sum RE:FSet(EAtt).(
			RS !={}&& RE !={})-> Request(
			RS, RE).PolicySet_policyset(
			RS, RE);	
	